'!TITLE "Denso robot program"

#Include "..\CGNX_API.pcs"

'
'HandEye Calibration (Finish)
'
Function CGNX_HEE(ByVal srvID As Integer, ByVal cam As Integer) As Variant
	Dim sendMsg As String
	Dim recvMsg As String

	sendMsg = Sprintf("HEE,%d",cam)
	recvMsg = CGNX_ExecuteCmd(srvID, sendMsg)
	CGNX_HEE = Split(recvMsg, ",")
End Function
